@model Technician
@{
    ViewData["Title"] = "Update";
    //NameValueCollection nvc = Request.Form;

}
<h4>@ViewData["Title"]</h4>

<p>testing the page</p>


<form asp-action="Index" asp-controller="UpdateIncident" method="post">
    <div asp-validation-summary="All" class="text-danger"></div>

    <div class="form-group">
        
        <select >
            @foreach(var technician in ViewBag.Technician)
            {
                <option name="techId" value="@technician.TechnicianId">@technician.Name</option>
            }
        </select> 
    </div>
    
    <button type="submit" class="btn btn-primary">Select</button>
    <!--<a asp-controller="UpdateIncident" asp-action="Index">Select</a> -->
</form>
